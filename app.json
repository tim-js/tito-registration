{
  "expo": {
    "name": "tito-checkin",
    "description": "Improved checkin app for ti.to events, based on the order number, which is an auto-increment.",
    "slug": "snack-SkajC7DxB",
    "privacy": "unlisted",
    "version": "1.0.3",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "assets/icons/revojs.png",
    "loading": {
      "icon": "https://d1wp6m56sqw74a.cloudfront.net/~assets/c9aa1be8a6a6fe81e20c3ac4106a2ebc",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4", "otf", "xml"]
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.revojs.registration",
      "buildNumber": "6",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan the attendee QR code from the event tickets."
      }
    },
    "android": {
      "package": "com.revojs.registration",
      "versionCode": 3,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "platforms": ["android", "ios"],
    "extra": {
      "eas": {
        "projectId": "c69323c0-b9bd-11e9-ae2d-d7c1cd1c14c7"
      }
    }
  }
}
